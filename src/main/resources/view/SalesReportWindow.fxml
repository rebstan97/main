<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.Scene?>
<?import javafx.scene.image.Image?>

<fx:root type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1">
  <icons>
    <Image url="@/images/sales.png" />
  </icons>
  <scene>
    <Scene>
      <VBox alignment="TOP_CENTER" prefHeight="290.0" prefWidth="465.0" spacing="10.0">
        <TableView fx:id="recordsTable" prefHeight="250.0" prefWidth="200.0">
          <columns>
            <TableColumn fx:id="itemName" prefWidth="150" text="Item Name" />
            <TableColumn fx:id="quantitySold" prefWidth="100" text="Quantity Sold" />
            <TableColumn fx:id="price" prefWidth="100" text="Price" />
            <TableColumn fx:id="revenue" prefWidth="100" text="Revenue" />
          </columns>
        </TableView>
        <Label fx:id="totalRevenue" alignment="BOTTOM_LEFT" prefHeight="18.0" prefWidth="708" text="Total Revenue: ">
        </Label>
      </VBox>
    </Scene>
  </scene>
</fx:root>
